{
  "expo": {
    "name": "Savemymeal",
    "slug": "savemymeal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "savemymeal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f7a5a"
    },
    "ios": {
      "icon": "./assets/ios_app_icon.png",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby vendors and delivery options.",
        "NSFaceIDUsageDescription": "Allow Face ID to quickly and securely log into your account.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.Savemymeal.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1f7a5a",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "package": "com.Savemymeal.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1f7a5a"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Savemymeal to use your location for nearby vendors and delivery."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": ""
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "stripePublishableKey": "pk_test_YOUR_STRIPE_KEY",
      "router": {},
      "eas": {
        "projectId": "e2a8b8a0-f3db-4258-b627-b81bcfec579d"
      }
    },
    "owner": "nosprodev"
  }
}
