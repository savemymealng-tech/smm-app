{
  "expo": {
    "name": "Savemymeal",
    "slug": "good-meal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "scheme": "Savemymeal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby vendors and delivery options.",
        "NSFaceIDUsageDescription": "Allow Face ID to quickly and securely log into your account.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.Savemymeal.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "package": "com.Savemymeal.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Savemymeal to use your location for nearby vendors and delivery."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": ""
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "stripePublishableKey": "pk_test_YOUR_STRIPE_KEY",
      "router": {},
      "eas": {
        "projectId": "c8f6a124-4827-4b11-bc2e-8c8a4e89dba3"
      }
    },
    "owner": "copy-ninja"
  }
}
